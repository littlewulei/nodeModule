#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_@ant-design_pro-cli@1.0.22@@ant-design/pro-cli/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_@ant-design_pro-cli@1.0.22@@ant-design/pro-cli/cli.js" "$@"
  ret=$?
fi
exit $ret
