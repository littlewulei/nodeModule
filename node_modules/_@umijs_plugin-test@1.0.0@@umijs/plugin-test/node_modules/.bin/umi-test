#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_@umijs_test@3.2.19@@umijs/test/bin/umi-test.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_@umijs_test@3.2.19@@umijs/test/bin/umi-test.js" "$@"
  ret=$?
fi
exit $ret
