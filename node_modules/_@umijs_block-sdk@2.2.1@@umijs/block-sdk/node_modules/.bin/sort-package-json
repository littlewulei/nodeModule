#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_sort-package-json@1.44.0@sort-package-json/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_sort-package-json@1.44.0@sort-package-json/cli.js" "$@"
  ret=$?
fi
exit $ret
